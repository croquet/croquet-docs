{
  "name": "croquet-doc-webshowcase",
  "version": "0.1.1",
  "description": "Croquet Metaverse Web Showcase Documentation",
  "scripts": {
    "start": "npm-watch",
    "build": "jsdoc -c jsdoc.json",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "./ShowcaseSpec.md",
        "./jsdoc.json"
      ],
      "ignore": [
        "dist/"
      ],
      "extensions": "js,json,html,css,tmpl,md,png,jpg,gif,svg,ttf,woff",
      "quiet": false
    }
  },
  "author": "Croquet Labs",
  "license": "Apache-2.0"
}
