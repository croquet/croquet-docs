{
  "source": {
    "include": [
        "./top.md",
        "./behavior-doc.js"
    ]
  },
  "opts": {
    "access": ["public"],
    "template": "../../clean-jsdoc-theme",
    "encoding": "utf8",
    "recurse": false,
    "verbose": true,
    "destination": "../../build/worldcore/behavior",
    "mainpagetitle": "Croquet Worldcore Behavior Overview",
    "theme_opts": {
        "theme": "light",
        "include_css": ["../../clean-jsdoc-theme/static/styles/index.css"]
    }
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
    "parser": "gfm",
    "hardwrap": false,
    "idInHeadings": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false,
      "includeDate": false,
      "useLongnameInNav": true,
      "staticFiles": {
        "includePattern": "\\.(png|jpg)$"
      }
    }
  }
}
