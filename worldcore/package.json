{
  "name": "croquet-doc-worldcore",
  "version": "1.0.0",
  "description": "Croquet Worldcore Documentation",
  "scripts": {
    "start": "npx npm-watch",
    "build": "../node_modules/.bin/jsdoc -c jsdoc.json",
    "watch": "npx npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "./top.md",
        "./root-doc.js",
        "./input-doc.js",
        "./mixin-doc.js",
        "./player-doc.js",
        "./render-doc.js",
        "./jsdoc.json",
        "../template"
      ],
      "ignore": [
        "build"
      ],
      "extensions": "js,json,html,css,tmpl,md",
      "quiet": false
    }
  },
  "author": "Croquet Corporation",
  "license": "UNLICENSED"
}
