{
  "name": "croquet-doc-worldcore",
  "version": "1.3.0",
  "description": "Croquet Worldcore Documentation",
  "scripts": {
    "start": "npm-watch",
    "doBuild": "./build.sh",
    "build": "../build.sh worldcore",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "./top.md",
        "./root-doc.js",
        "./mixin-doc.js",
        "./player-doc.js",
        "./jsdoc.json",
        "../template"
      ],
      "ignore": [
        "dist/"
      ],
      "extensions": "js,json,html,css,tmpl,md,png,jpg,gif,svg,ttf,woff",
      "quiet": false
    }
  },
  "author": "Croquet Labs",
  "license": "UNLICENSED",
  "dependencies": {
    "fs-extra": "^10.0.0"
  }
}
