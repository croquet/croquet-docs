{
  "name": "croquet-doc-worldcore",
  "version": "1.0.7",
  "description": "Croquet Worldcore Documentation",
  "scripts": {
    "start": "npx npm-watch",
    "build": "npx jsdoc -c jsdoc.json; ./build-dirs.sh",
    "watch": "npx npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "./top.md",
        "./root-doc.js",
        "./mixin-doc.js",
        "./player-doc.js",
        "./jsdoc.json",
        "../template"
      ],
      "ignore": [
        "build"
      ],
      "extensions": "js,json,html,css,tmpl,md",
      "quiet": false
    }
  },
  "author": "Croquet Corporation",
  "license": "UNLICENSED",
  "dependencies": {
    "fs-extra": "^10.0.0"
  }
}
