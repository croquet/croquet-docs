{
  "source": {
    "include": [
        "./top.md",
        "./widget-doc.js"
    ]
  },
  "opts": {
    "access": ["public"],
    "template": "../../template",
    "encoding": "utf8",
    "recurse": false,
    "verbose": true,
    "destination": "../build/worldcore/widget"
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
    "parser": "gfm",
    "hardwrap": false,
    "idInHeadings": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false,
      "includeDate": false,
      "useLongnameInNav": true,
      "staticFiles": {
        "includePattern": "\\.(png|jpg)$"
      }
    }
  },
  "docdash": {
    "static": true,
    "sort": false,
    "disqus": "",
    "meta": {
      "title": "Croquet Worldcore Widget Overview",
      "description": "Documentation for the Croquet Worldcore Widget Framework",
      "keyword": "croquet,"
    },
    "search": true,
    "collapse": true,
    "typedefs": true,
    "removeQuotes": "none",
    "scripts": [],
    "menuHome": "Home: <b>Croquet Worldcore Widget</b>"
  }
}
