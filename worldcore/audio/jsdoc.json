{
  "source": {
    "include": [
        "./top.md",
        "./audio-doc.js"
    ]
  },
    "opts": {
    "access": ["public"],
    "encoding": "utf8",
    "recurse": false,
    "verbose": true,
    "destination": "../../build/worldcore/audio",
    "template": "../../node_modules/clean-jsdoc-theme",
    "theme_opts": {
        "theme": "light",
        "static_dir": ["../static"],
        "include_css": ["../static/styles/index.css"]
    }
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
    "parser": "gfm",
    "hardwrap": false,
    "idInHeadings": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false,
      "includeDate": false,
      "useLongnameInNav": true,
      "staticFiles": {
        "includePattern": "\\.(png|jpg)$"
      }
    }
  }
}
