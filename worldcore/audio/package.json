{
  "name": "croquet-doc-worldcore-audio",
  "version": "0.5.0",
  "description": "Croquet Worldcore Audio Documentation",
  "scripts": {
    "start": "npx npm-watch",
    "build": "npx jsdoc -c jsdoc.json",
    "watch": "npx npm-watch",
    "generate-docs": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose"

  },
  "opts": {
    "encoding": "utf8",
    "readme": "./README.md",
    "destination": "docs/",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
        "theme": "dark"
  }
},
  "watch": {
    "build": {
      "patterns": [
        "./top.md",
        "./audio-doc.js",
        "./jsdoc.json"
      ],
      "ignore": [
        "build"
      ],
      "extensions": "js,json,html,css,tmpl,md",
      "quiet": false
    }
  },
  "author": "Croquet Corporation",
  "license": "UNLICENSED"
}
