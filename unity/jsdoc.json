{
  "source": {
    "include": [
        "../../m4u-tutorials/docs/unity-doc.js",
        "../../m4u-tutorials/docs/remaining_from_old.md",
        "../../m4u-tutorials/docs/home.md"
    ]
  },
  "opts": {
    "access": ["public"],
    "encoding": "utf8",
    "recurse": false,
    "verbose": true,
    "destination": "../dist/unity/",
    "mainpagetitle": "Multisynq For Unity Documentation",
    "theme_opts": {
      "home": {
        "name": "Home",
        "url": "./index.html",
        "target": "_self"
      },
      "extra_sidebar_items": [
        {
          "title": "The Build Assistant",
          "path": "../../m4u-tutorials/docs/build_assistant"
        },
        {
          "title": "[SynqVar] & [SynqRPC]",
          "path": "../../m4u-tutorials/docs/synqvar_synqrpc"
        },
        {
          "title": "Migration",
          "path": "../../m4u-tutorials/docs/migration"
        },
        {
          "title": "Deterministic (Advanced)",
          "path": "../../m4u-tutorials/docs/advanced"
        },
        {
          "title": "Advanced Projects",
          "path": "../../m4u-tutorials/docs/projects"
        }
      ],
      "extra_directories": [
        {
          "path": "../../m4u-tutorials/docs/images",
          "destination": "images"
        },
        {
          "path": "../../m4u-tutorials/docs/videos",
          "destination": "videos"
        }
      ]
    }
  },
  "plugins": [
    "plugins/markdown"
  ],
  "markdown": {
    "parser": "gfm",
    "hardwrap": false,
    "idInHeadings": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false,
      "includeDate": false,
      "useLongnameInNav": true,
      "staticFiles": {
        "includePattern": "\\.(png|jpg|svg|gif)$"
      }
    }
  },
  "prism": {
    "languages": [
      "csharp",
      "javascript",
      "css",
      "markup"
    ]
  }
}
