{
  "source": {
    "include": [
        "../../croquet-for-unity-tutorials/docs/unity-doc.js",
        "../../croquet-for-unity-tutorials/docs/top.md",
        "../../croquet-for-unity-tutorials/docs/guide.md"
    ]
  },
  "opts": {
    "access": ["public"],
    "encoding": "utf8",
    "recurse": false,
    "verbose": true,
    "destination": "../dist/unity/",
    "mainpagetitle": "Croquet For Unity Documentation",
    "theme_opts": {
      "extra_sidebar_items": [
        {
          "title": "A.P.I. Reference",
          "path": "../../croquet-for-unity-tutorials/docs/api"
        },
        {
          "title": "Projects",
          "path": "../../croquet-for-unity-tutorials/docs/projects"
        },
        {
          "title": "Tutorials",
          "path": "../../croquet-for-unity-tutorials/docs/tutorials"
        },
        {
          "title": "Photon Migration Guide",
          "path": "../../croquet-for-unity-tutorials/docs/photon_migration_guide.md"
        },
        {
          "title": "Build Assistant Window",
          "path": "../../croquet-for-unity-tutorials/docs/build_assistant_window.md"
        }
      ],
      "extra_directories": [
        {
          "path": "../../croquet-for-unity-tutorials/docs/images",
          "destination": "images"
        }
      ]
    }
  },
  "plugins": [
    "plugins/markdown",
    "jsdoc-plugin-typescript"
  ],
  "markdown": {
    "parser": "gfm",
    "hardwrap": false,
    "idInHeadings": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false,
      "includeDate": false,
      "useLongnameInNav": true,
      "staticFiles": {
        "includePattern": "\\.(png|jpg|svg|gif)$"
      }
    }
  },
  "prism": {
    "languages": [
      "csharp",
      "javascript",
      "css",
      "markup"
    ]
  }
}
