{
  "source": {
    "include": [
        "../../croquet-react/docs/top.md",
        "../../croquet-react/docs/react-doc.js"
    ]
  },
  "opts": {
    "access": ["public"],
    "tutorials": "../../croquet-react/docs/tutorials",
    "template": "../jsdoc-theme",
    "encoding": "utf8",
    "recurse": false,
    "verbose": true,
    "destination": "../dist/multisynq-react/",
    "mainpagetitle": "Multisynq React Bindings @MULTISYNQ_VERSION@ References",
    "theme_opts": {
      "include_css": [
        "../jsdoc-theme/static/styles/clean-jsdoc-theme-base.css", 
        "../jsdoc-theme/static/styles/multisynq.css"
      ],
      "theme": "light",
      "logo": "multisynq_vertical_white.svg",
      "title": "<div class=\"navbar-heading\" id=\"navbar-heading\"><a href=\"index.html\"><img src=\"images/multisynq_vertical_blue.svg\"/></a></div>",
      "subdirectory": "../multisynq-react",
      "search": true
    }
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
    "parser": "gfm",
    "hardwrap": false,
    "idInHeadings": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false,
      "includeDate": false,
      "useLongnameInNav": true,
      "staticFiles": {
        "includePattern": "\\.(png|jpg|svg|gif)$"
      }
    }
  }
}
