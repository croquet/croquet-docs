{
  "name": "croquet-doc-react",
  "version": "1.4.1",
  "description": "Croquet React Bindings",
  "scripts": {
    "start": "npm-watch",
    "build": "../node_modules/.bin/jsdoc -c jsdoc.json",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "../../croquet-react/docs/top.md",
        "../../croquet-react/docs/react-doc.js",
        "../../croquet-react/docs/tutorials/*",
        "./jsdoc.json"
      ],
      "extensions": "js,json,html,css,tmpl,md",
      "quiet": false
    }
  },
  "author": "Croquet Corporation",
  "license": "UNLICENSED"
}
