{
  "source": {
    "include": [
        "../../croquet-react/docs/top.md",
        "../../croquet-react/docs/react-doc.js"
    ]
  },
  "opts": {
    "access": ["public"],
    "tutorials": "../../croquet-react/docs/tutorials",
    "encoding": "utf8",
    "recurse": false,
    "verbose": true,
    "mainpagetitle": "Croquet React Bindings @CROQUET_VERSION@",
    "theme_opts": {
      "extra_md": [
        {
          "title": "v2.0.0 Migration Guide",
          "path": "../../croquet-react/docs/v2.0.0_migration_guide.md"
        }
      ]
    }
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
    "parser": "gfm",
    "hardwrap": false,
    "idInHeadings": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false,
      "includeDate": false,
      "useLongnameInNav": true,
      "staticFiles": {
        "includePattern": "\\.(png|jpg|svg|gif)$"
      }
    }
  }
}
