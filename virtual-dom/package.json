{
  "name": "croquet-doc-virtual-dom",
  "version": "1.0.7",
  "description": "Croquet Virtual DOM Documentation",
  "scripts": {
    "start": "npx npm-watch",
    "build": "../node_modules/.bin/jsdoc -c jsdoc.json",
    "watch": "npx npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "./top.md",
        "./element-doc.js",
        "./tutorials/*",
        "./jsdoc.json"
      ],
      "ignore": [
        "build"
      ],
      "extensions": "js,json,html,css,tmpl,md",
      "quiet": false
    }
  },
  "author": "Croquet Corporation",
  "license": "UNLICENSED"
}
