{
  "name": "croquet-doc-virtual-dom",
  "version": "1.0.8",
  "description": "Croquet Virtual DOM Documentation",
  "scripts": {
    "start": "npm-watch",
    "doBuild": "jsdoc -c jsdoc.json",
    "build": "../build.sh virtual-dom",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "./top.md",
        "./element-doc.js",
        "./tutorials/*",
        "./jsdoc.json"
      ],
      "ignore": [
        "dist/"
      ],
      "extensions": "js,json,html,css,tmpl,md,png,jpg,gif,svg,ttf,woff",
      "quiet": false
    }
  },
  "author": "Croquet Labs",
  "license": "UNLICENSED"
}
