{
  "name": "croquet-doc-virtual-dom",
  "version": "0.5.0",
  "description": "Croquet Virtual DOM Documentation",
  "scripts": {
    "start": "npm-watch",
    "build": "jsdoc -c jsdoc.json -d build",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "../../../authoring/apiDoc/top.md",
        "../../../authoring/apiDoc/element-doc.js",
        "../../../authoring/apiDoc/tutorials/*",
        "./jsdoc.json"
      ],
      "ignore": [
        "build"
      ],
      "extensions": "js,json,html,css,tmpl,md",
      "quiet": false
    }
  },
  "author": "Croquet Corporation",
  "license": "UNLICENSED"
}
