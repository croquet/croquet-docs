{
  "name": "croquet-client-docs",
  "version": "1.1.0",
  "description": "Croquet Client Documentation",
  "scripts": {
    "start": "npm-watch",
    "build": "jsdoc -c jsdoc.json",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "..",
        "../../wonderland/croquet/teatime"
      ],
      "ignore": [
        "dist/"
      ],
      "extensions": "js,json,html,css,tmpl,md,png,jpg,gif,svg,ttf,woff",
      "quiet": false
    }
  },
  "author": "Croquet Labs",
  "license": "UNLICENSED"
}
