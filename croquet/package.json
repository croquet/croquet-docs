{
  "name": "croquet-client-docs",
  "version": "1.0.4",
  "description": "Croquet Client Documentation",
  "scripts": {
    "start": "npm-watch",
    "build": "jsdoc -c jsdoc.json -d build",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
          "README.md",
          "../../teatime/index.js",
          "../../teatime/src/model.js",
          "../../teatime/src/view.js",
          "../../teatime/src/session.js",
          "tutorials",
          "../template"
      ],
      "ignore": [
        "build"
      ],
      "extensions": "js,json,html,css,tmpl,md",
      "quiet": false
    }
  },
  "author": "Croquet Corporation",
  "license": "UNLICENSED"
}
