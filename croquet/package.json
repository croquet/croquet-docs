{
  "name": "croquet-client-docs",
  "version": "2.0.0",
  "description": "Croquet Client Documentation",
  "scripts": {
    "start": "npm-watch",
    "doBuild": "jsdoc -c jsdoc.json",
    "build": "ln -sf ../../croquet/docs/tutorials .; ../build.sh croquet",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "..",
        "../../croquet/docs",
        "../../croquet/packages/croquet/teatime"
      ],
      "ignore": [
        "dist/"
      ],
      "extensions": "js,json,html,css,tmpl,md,png,jpg,gif,svg,ttf,woff",
      "quiet": false
    }
  },
  "author": "Croquet Labs",
  "license": "Apache-2.0"
}
