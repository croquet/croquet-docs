{
  "source": {
    "include": [
      "README.md",
      "../../wonderland/croquet/teatime/index.js",
      "../../wonderland/croquet/teatime/src/model.js",
      "../../wonderland/croquet/teatime/src/view.js",
      "../../wonderland/croquet/teatime/src/session.js"
    ]
  },
  "opts": {
    "access": ["public"],
    "tutorials": "tutorials",
    "encoding": "utf8",
    "recurse": false,
    "verbose": true,
    "mainpagetitle": "Croquet @CROQUET_VERSION@",
    "theme_opts": {
        "theme": "light"
    }
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
    "parser": "gfm",
    "hardwrap": false,
    "idInHeadings": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false,
      "includeDate": false,
      "useLongnameInNav": true,
      "staticFiles": {
        "include": [
          "tutorials"
        ],
        "includePattern": "\\.(png|jpg|svg|gif)$"
      }
    }
  }
}
